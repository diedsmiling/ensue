{
  "name": "ensue",
  "version": "1.0.2",
  "description": "",
  "main": "dist/ensue.es5.js",
  "jsnext:main": "dist/ensue.js",
  "scripts": {
    "test:tap": "tap ensue.test.js --nyc-arg=--require --nyc-arg=babel-polyfill --nyc-arg=--require --nyc-arg=babel-register --100",
    "test": "npm run test:tap",
    "coverage": "nyc tap ensue.test.js --nyc-arg=--require --nyc-arg=babel-polyfill --nyc-arg=--require --nyc-arg=babel-register --100 --coverage-report=html",
    "transpile": "babel ensue.js --out-file ensue.es5.js",
    "clean": "rimraf ensue.es5.js & rimraf dist",
    "prepublish": "npm run build",
    "postpublish": "npm run clean",
    "prebuild":"npm run transpile",
    "build": "rollup -c & rollup -c --environment BUILD:es5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zerobias/ensue.git"
  },
  "author": "Zero Bias",
  "license": "MIT",
  "dependencies": {
    "ramda": "^0.22.1"
  },
  "devDependencies": {
    "nyc": "^8.3.0",
    "rollup": "^0.36.1",
    "rollup-plugin-cleanup": "^0.1.4",
    "rollup-plugin-commonjs": "^5.0.4",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-uglify": "^1.0.1",
    "tap": "^7.1.2",
    "uglify-js": "^2.7.3"
  }
}
